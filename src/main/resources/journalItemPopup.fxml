<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="500.0" prefWidth="300.0">
      <children>
         <VBox fx:id="selectionPane" alignment="CENTER" prefHeight="500.0" prefWidth="300.0">
            <children>
               <Label fx:id="header" prefHeight="35.0" prefWidth="277.0" text="Create new Journal Item">
                  <font>
                     <Font size="24.0" />
                  </font>
               </Label>
               <ComboBox fx:id="itemTypeChoice" prefHeight="25.0" prefWidth="200.0" />
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>
